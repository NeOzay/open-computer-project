{
	"workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
	"editor.semanticHighlighting.enabled": true,
	"[json]": {
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"material-icon-theme.folders.theme": "specific",
	"editor.glyphMargin": false,
	"editor.folding": true,
	"editor.lineDecorationsWidth": 0, // undocumented
	//"editor.lineNumbersMinChars": 5,  // undocumented
	"editor.columnSelection": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true
	},
	"Lua.workspace.ignoreDir": [
		".vscode",
		//"doc",
		"openOS",
		//"programs.cfg"
	],
	"Lua.runtime.pathStrict": false,
	"Lua.hint.setType": true,
	"Lua.misc.parameters": [
	//"--develop=true",
	//"--dbgport=11428"
	],
	"Lua.diagnostics.disable": [
		"lowercase-global",
		//"need-check-nil",
		//"duplicate-set-field",
		//"unbalanced-assignments"
	],

	//"Lua.misc.executablePath": "C:\\Users\\colpa\\IdeaProjects\\lua-language-server\\bin\\lua-language-server.exe",
	"vscode-lua-format.configPath": ".vscode/lua-format.txt",
	"files.associations": {
		"*.cfg": "properties"
	},
	"vscode-lua-minify.flavour": "lua-format",
	"vscode-lua-minify.renameVariables": true,
	"Lua.runtime.version": "Lua 5.3",
	"Lua.workspace.library": [
		"C:/Users/colpa/IdeaProjects/LuaLs-addon/OpenComputer-def/library"
	],
	"Lua.diagnostics.globals": [
		"vim",
		"computer",
		"component"
	],

}